{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.5.10",
    "antd": "4.19.5",
    "i18next": "21.6.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.16.7",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@craco/craco": "6.4.5",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "apollo": "2.33.10",
    "typescript": "4.6.3"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen_types": "apollo codegen:generate ./src/_gqlTypes --target=typescript --no-addTypename --outputFlat --passthroughCustomScalars --excludes='**/*.test.*' && cd ../../ && yarn prettier --write apps/portal/src/_gqlTypes",
    "create_comp": "scripts/createComponent.js",
    "tscheck": "tsc --noEmit -p .",
    "tscheck:watch": "tsc -w --noEmit -p ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
