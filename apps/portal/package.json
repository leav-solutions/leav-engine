{
  "name": "portal",
  "version": "1.0.0-beta2",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@leav/ui": "workspace:*",
    "antd": "5.1.5",
    "cross-fetch": "3.1.5",
    "i18next": "21.6.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.16.7",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "3.1.0",
    "apollo": "2.34.0",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "ts-jest": "29.0.5",
    "ts-jest-mock-import-meta": "1.0.0",
    "typescript": "4.8.3",
    "vite": "4.1.1",
    "vite-jest": "0.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "gen_types": "apollo codegen:generate ./src/_gqlTypes --target=typescript --no-addTypename --outputFlat --passthroughCustomScalars --excludes='**/*.test.*' && cd ../../ && yarn prettier --write apps/portal/src/_gqlTypes",
    "create_comp": "scripts/createComponent.js",
    "tscheck": "tsc --noEmit -p .",
    "tscheck:watch": "tsc -w --noEmit -p ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stableVersion": "0.1.0"
}
