{
    "testEnvironment": "node",
    "moduleFileExtensions": [
        "ts",
        "js"
    ],
    "transform": {
        "\\.(ts|tsx)$": [
            "ts-jest",
            {
                "isolatedModules": true
            }
        ]
    },
    "testRegex": ".spec.(ts)$",
    "setupFiles": [
        "<rootDir>/src/__tests__/setup.ts",
        "<rootDir>/src/__tests__/jestUtils.ts"
    ],
    "moduleNameMapper": {
        "exceljs": "<rootDir>/../../node_modules/exceljs/dist/exceljs.js"
    }
}