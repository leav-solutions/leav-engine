{
    "moduleFileExtensions": [
        "ts",
        "js"
    ],
    "transform": {
        "\\.(ts|tsx)$": [
            "ts-jest",
            {
                "isolatedModules": true
            }
        ]
    },
    "testRegex": "src/__tests__/e2e/indexationManager/.*(.test.ts)$",
    "globalSetup": "./src/__tests__/e2e/indexationManager/globalSetupHook.ts",
    "setupFiles": [
        "<rootDir>/src/__tests__/setup.ts",
        "<rootDir>/src/__tests__/jestUtils.ts"
    ]
}