{
    "name": "core",
    "version": "1.0.0-beta14",
    "description": "",
    "scripts": {
        "start": "node .",
        "start:watch": "npm run start:dev",
        "start:files": "ts-node-dev --inspect=0.0.0.0:9230 --respawn src/ --filesManager",
        "start:indexation": "ts-node-dev --respawn src/ --indexationManager",
        "start:tasksMaster": "ts-node-dev --inspect=0.0.0.0:9232 --respawn src/ --tasksManager=master",
        "start:tasksWorker": "ts-node-dev --inspect=0.0.0.0:9234 --respawn src/ --tasksManager=worker",
        "start:dev": "ts-node-dev --inspect=0.0.0.0:9229 --respawn src/ --server",
        "test:watch": "jest -c jest-unit-config.json --watch",
        "test": "jest -c jest-unit-config.json",
        "test:ci": "jest -c jest-unit-config.json",
        "test:commit": "jest -c jest-unit-config.json --maxWorkers=50% --onlyChanged ",
        "test:e2e": "npm run test:e2e:api",
        "test:e2e:api": "NODE_ENV=test jest -c jest-e2e-api-config.json --maxWorkers=2 --forceExit --detectOpenHandles",
        "test:e2e:filesManager": "NODE_ENV=test jest -c jest-e2e-filesManager-config.json --forceExit --detectOpenHandles",
        "test:e2e:indexationManager": "NODE_ENV=test jest -c jest-e2e-indexationManager-config.json --forceExit --detectOpenHandles",
        "lint": "eslint --fix src/",
        "prettier": "prettier --write 'src/**/*.ts'",
        "db:migrate": "CORE_MODE=migrate node .",
        "db:migrate:dev": "CORE_MODE=migrate ts-node src/",
        "importConfig": "ts-node src/ importConfig --clear",
        "importData": "ts-node src/ importData",
        "buildapps": "node . build-apps",
        "buildapps:dev": "ts-node src/ build-apps",
        "benchmark": "ts-node src/ benchmark",
        "build": "scripts/build.js",
        "tscheck": "tsc --noEmit -p .",
        "tscheck:watch": "tsc -w --noEmit -p tsconfig.json"
    },
    "main": "dist/index.js",
    "author": "",
    "license": "LGPL3",
    "devDependencies": {
        "@types/amqplib": "0.8.2",
        "@types/cacache": "15.0.1",
