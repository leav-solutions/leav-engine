{
    "moduleFileExtensions": [
        "ts",
        "js"
    ],
    "transform": {
        "\\.(ts|tsx)$": [
            "ts-jest",
            {
                "isolatedModules": true
            }
        ]
    },
    "testRegex": ".spec.(ts)$",
    "setupFiles": [
        "<rootDir>/src/__tests__/jestUtils.ts"
    ],
    "testTimeout": 30000
}