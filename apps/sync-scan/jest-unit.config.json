{
    "moduleFileExtensions": [
        "ts",
        "js"
    ],
    "transform": {
        "\\.(ts|tsx)$": [
            "ts-jest",
            {
                "isolatedModules": true
            }
        ]
    },
    "testRegex": "src\/__tests__\/unit\/.*(.test.ts)$",
    "roots":  ["<rootDir>/src/__tests__/unit"]
}
