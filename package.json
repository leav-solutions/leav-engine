{
  "name": "leav-monorepo",
  "version": "1.0.0-beta",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*"
    ]
  },
  "scripts": {
    "test": "CI=true node ./scripts/testChangedProjects.js",
    "lint": "eslint --ext=ts,tsx --fix apps/ libs/",
    "tscheck": "yarn workspaces foreach -vp run tscheck",
    "license:check": "license-check-and-add check -f license-check-config.json",
    "license:add": "license-check-and-add add -f license-check-config.json",
    "license:remove": "license-check-and-add remove -f license-check-config.json",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "1.1.3",
    "ajv": "8.10.0",
    "eslint": "7.16.0",
    "prettier": "2.2.1",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@types/testing-library__jest-dom": "5.9.5",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "babel-eslint": "10.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "6.0.0",
    "jest-watch-typeahead": "2.2.0",
    "license-check-and-add": "3.0.5",
    "lint-staged": "10.5.3"
  },
  "resolutions": {
    "fs-capacitor": "6.2.0",
    "graphql-upload": "11.0.0",
    "graphql": "15",
    "data-studio:babel-jest": "26",
    "data-studio:jest": "26.6.0",
    "react-error-overlay": "6.0.9",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "semver": "7.3.7",
    "git-url-parse": "13"
  },
  "lint-staged": {
    "*.{ts, js, tsx, jsx}": [
      "prettier --write",
      "eslint --fix",
      "license-check-and-add add -f license-check-config.json"
    ]
  },
  "packageManager": "yarn@3.2.3",
  "stableVersion": "0.0.0"
}
